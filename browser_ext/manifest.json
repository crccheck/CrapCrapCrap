{
  "manifest_version": 2,
  "name": "CrapCrapCrap",
  "version": "0.1.0",
  "description": "Track prices on sites you care about",
  "permissions": [
    "http://localhost/*",
    "https://tracker.craptobuy.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.bigbadtoystore.com/*"],
      "js": ["crap.js"]
    }
  ]
}
